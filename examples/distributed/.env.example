# Docker Compose environment variables for distributed Elacsym

# MinIO credentials
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin

# Shared storage bucket
ELACSYM_S3_BUCKET=elacsym-data
ELACSYM_S3_REGION=us-east-1
ELACSYM_S3_ENDPOINT=http://minio:9000

# WAL prefix for multi-tenant buckets
ELACSYM_WAL_PREFIX=production

# Default image tag (override to pin releases)
ELACSYM_IMAGE=elacsym:latest
